int main() {
    int i, sum = 0;
    
    for (i = 1; i <= 10; i++) {
        sum = sum + i;
    }

    for (int j = 0; j < 5; j++) {
        sum = sum + j * 2;
    }
    
    for (int x = 0; x < 3; x++) {
        for (int y = 0; y < 3; y++) {
            sum = sum + x * y;
        }
    }
    
    return sum;
}
